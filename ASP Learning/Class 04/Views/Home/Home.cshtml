
@{
    ViewBag.Title = "Home";
}

<h2>Home</h2>

<h2>@ViewBag.Message</h2>

<h2>@ViewBag.CurrentDate</h2>


<ul>

    @{
        foreach (string i in ViewBag.FruitsArray)
        {
            <li>@i</li>
        }
    }

</ul>

<ul>

    @{
        foreach (string i in ViewBag.SportsList)
        {
            <li>@i</li>
        }
    }

</ul>

@{ 
    var e = ViewBag.employee;
}

<p>@e.EmpId</p>
<p>@e.EmpName</p>
<p>@e.EmpDesignation</p>